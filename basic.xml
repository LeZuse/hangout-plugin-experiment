<?xml version="1.0" encoding="UTF-8"?>
<Module>
  <ModulePrefs title="@SusanPotter's basic app for testing" height="300" width="600">
  <Require feature="dynamic-height"/>
  <Require feature="views"/>
</ModulePrefs>
<Content type="html"><![CDATA[
  <link type="text/css" rel="stylesheet"
    href="https://lezuse.github.io/hangouts-plugin-experiment/styles.css" />

  <div id="container">
    <div onclick={mute(true)}>mute</div>
    <div onclick={mute(false)}>unmute</div>
  </div>

  <script type="text/javascript" src="//www.google.com/jsapi"></script>
  <script type="text/javascript">google.load("jquery", "1.6.4");</script>
  <script type="text/javascript"
    src="//hangoutsapi.talkgadget.google.com/hangouts/api/hangout.js?v=0.1"></script>

  <script type="text/javascript"
    src="https://lezuse.github.io/hangouts-plugin-experiment/basic.js"></script>
]]></Content>
</Module>
